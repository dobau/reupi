# INSTALL
docker pull sath89/oracle-12c
ocker run -d -p 8080:8080 -p 1521:1521 sath89/oracle-12c

# DATABASE
hostname: localhost
port: 1521
sid: xe
service name: xe
username: system
password: oracle

# APEX
http://localhost:8080/apex
workspace: INTERNAL
user: ADMIN
password: 0Racle$

# EM
http://localhost:8080/em
user: sys
password: oracle
connect as sysdba: true
